---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>Serg Podtynnyi</title>
  <link href="http://podtynnyi.com/atom.xml" rel="self"/>
  <link href="http://podtynnyi.com/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://podtynnyi.com/</id>
  <author>
    <name>Serg Podtynnyi</name>
    <email>serg@podtynnyi.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://podtynnyi.com{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://podtynnyi.com{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}

</feed>
